{
  "manifest_version": 2,

  "name": "Page Switcher for Google Search.",
  "description": "Allows user to move between the previous and the next page in google search.",
  "version": "1.0",
  "background": {
   
    "scripts": ["eventPage.js"],
    "persistent": false
  
	},
  "page_action" :
  {
    "default_icon" : "icon-16.png",
    "default_title" : "Click here!",
	"default_popup": "popup.html"
  },

  "icons" : {
    "32" : "icon-32.png"
  },
  
  "commands": {
    "next_page": {
      "suggested_key": { 
      "default": "Alt+Shift+Right", 
      "mac": "MacCtrl + Shift + Right"
    },
      "description": "To open next page in google search."
    },

    "previous_page": {
      "suggested_key": { "default": "Alt+Shift+Left" ,
        "mac": "MacCtrl + Shift + Left"
      },
      "description": "To open previous page in google search."
    }
  },

  "permissions": [
    "activeTab",
    "storage",
	  "tabs",
    "declarativeContent"
  ],

